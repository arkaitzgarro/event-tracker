  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arkaitzgarro/event-tracker/issues"
  },
  "homepage": "https://github.com/arkaitzgarro/event-tracker#readme",
  "devDependencies": {
    "eslint": "4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "jasmine": "2.7.0",
    "jasmine-spec-reporter": "4.1.1",
    "nodemon": "1.11.0"
  },
  "dependencies": {
    "pg": "7.0.2",
    "pg-hstore": "2.3.2",
    "pm2": "2.6.1",
